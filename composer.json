{
    "name": "chiwex/laravel-referral",
    "description": "A Referral System With Laravel",
    "keywords": ["referral", "referral-system", "affiliate-tracking"],
    "license": "MIT",
    "authors": [{
        "name": "chiwex",
        "email": "donchiwexco@gmail.com"
    }],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "laravel/laravel": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chiwex\\Referral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chiwex\\Referral\\ReferralServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
